{
  "name": "eslint-plugin-crc",
  "description": "Document, analyze, and refactor JavaScript codebases with auto-generated CRC (Class-Responsibility-Collaborator) Models.",
  "version": "0.1.5-development",
  "author": {
    "name": "Greg Swindle",
    "email": "greg@swindle.net",
    "url": "https://github.com/gregswindle"
  },
  "bin": {
    "crc": "lib/cli.js"
  },
  "bugs": {
    "url": "https://github.com/gregswindle/eslint-plugin-crc/issues"
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "build",
      "chore",
      "ci",
      "docs",
      "feat",
      "fix",
      "perf",
      "refactor",
      "revert",
      "style",
      "test"
    ],
    "scope": "\\S+.*"
  },
  "contributors": [],
  "dependencies": {
    "boom": "7.1.1",
    "bunyan": "1.8.12",
    "catharsis": "0.8.9",
    "chalk": "2.3.1",
    "compromise": "11.4.0",
    "cryptiles": "4.1.1",
    "debug": "3.1.0",
    "doctrine": "2.1.0",
    "documentation": "5.4.0",
    "escomplex": "2.0.0-alpha",
    "eslint": "4.18.1",
    "eslint-scope": "3.7.1",
    "espree": "3.5.3",
    "esquery": "1.0.0",
    "estraverse": "4.2.0",
    "fs-extra": "5.0.0",
    "hawk": "7.0.7",
    "lodash": "4.17.5",
    "markdown-it": "8.4.1",
    "markdown-it-toc-and-anchor": "4.1.2",
    "markdown-toc": "1.2.0",
    "meow": "4.0.0",
    "request": "2.83.0",
    "requireindex": "1.2.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "1.0.1",
    "@semantic-release/git": "3.0.1",
    "@semantic-release/npm": "3.0.2",
    "ajv": "6.4.0",
    "ajv-keywords": "3.2.0",
    "chai": "4.1.2",
    "codacy-coverage": "3.0.0",
    "commitplease": "3.2.0",
    "common-vocabulary": "1.0.1",
    "coveralls": "3.0.0",
    "docdash": "0.4.0",
    "eslint": "4.19.1",
    "eslint-config-crockford": "2.0.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-scanjs": "1.0.0-beta4",
    "eslint-config-standard": "11.0.0",
    "eslint-config-xo": "0.20.1",
    "eslint-config-xo-space": "0.18.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsdoc": "3.6.3",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-no-unsafe-innerhtml": "1.0.16",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-scanjs-rules": "0.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "3.1.0",
    "eslint-plugin-unicorn": "4.0.3",
    "eslint-plugin-xss": "0.1.9",
    "fixpack": "2.3.1",
    "hoek": "5.0.3",
    "husky": "^0.14.3",
    "jest": "22.4.3",
    "jsdoc": "3.5.5",
    "lec": "^1.0.1",
    "lint-staged": "7.0.5",
    "markdown-magic": "0.1.20",
    "markdown-magic-dependency-table": "1.3.2",
    "markdown-magic-package-scripts": "1.2.1",
    "mocha": "5.0.1",
    "nodemon": "1.15.1",
    "nsp": "3.2.1",
    "nyc": "11.4.1",
    "prettier": "1.12.1",
    "properties-parser": "0.3.1",
    "semantic-release": "13.4.1",
    "sinon": "4.3.0",
    "sinon-chai": "2.14.0",
    "snyk": "1.69.7",
    "travis-deploy-once": "^4.3.4"
  },
  "directories": {
    "doc": "./docs",
    "lib": "./lib",
    "test": "./tests"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "greenkeeper": {
    "commitMessages": {
      "dependencyPin": "fix(${dependency}): pin to ${oldVersion}",
      "dependencyUpdate": "fix(${dependency}): update to version ${version}",
      "devDependencyPin": "fix(${dependency}): pin to ${oldVersion}",
      "devDependencyUpdate": "fix(${dependency}): update to version ${version}",
      "initialBadge": "docs(greenkeeper): add greenkeeper badge",
      "initialBranches": "build(dependencies): whitelist greenkeeper branches",
      "initialDependencies": "fix(pkg): update dependencies"
    }
  },
  "homepage": "https://github.com/gregswindle/eslint-plugin-crc/",
  "keywords": [
    "ast",
    "cli",
    "code-analysis",
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "javascript",
    "javascript-tools",
    "refactor",
    "refactoring",
    "refactoring-tools"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "npm run lint:js",
      "git add"
    ],
    "*.json": [
      "npm run lint:json",
      "git add"
    ],
    "*.md": [
      "npm run lint:md",
      "npm run docs",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "maintainers": [
    {
      "email": "greg@swindle.net",
      "name": "gregswindle"
    }
  ],
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "exclude": [
      "**/descriptors/**",
      "**/tests/**",
      "reports/**"
    ],
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "./docs/reports/coverage"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "publish": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gregswindle/eslint-plugin-crc.git"
  },
  "scripts": {
    "docs": "npm run docs2html && npm run docs:markdown",
    "docs2html": "jsdoc -c config/jsdoc.conf.json",
    "docs:glossary": "common-vocabulary",
    "docs:json": "jsdoc -c config/jsdoc.conf.json -X",
    "docs:markdown": "md-magic --path '{.github/*.md,**/*.md}' '**/*.md' --ignore 'node_modules' --config ./config/markdown.config.js",
    "lint": "npm run lint:js && npm run lint:json && npm run lint:manifest && npm run lint:md",
    "lint:js": "eslint -c .github/config/.eslintrc.yml --ext .js . --fix",
    "lint:json": "prettier ./**/*.json --ignore-path '.github/config/.prettierignore' --write",
    "lint:manifest": "fixpack",
    "lint:md": "prettier ./**/*.md -prose-wrap always --write",
    "postversion": "node ./.ci/bump/ && git add sonar-project.properties && git commit --amend && git push && git push --tags",
    "precommit": "lint-staged",
    "prepublishOnly": "lec lib/cli.js -c LF && npm run security",
    "pretest": "npm run security && npm run lint",
    "preversion": "npm test && npm run security",
    "push": "git push origin master && git status",
    "security": "./node_modules/.bin/nsp check --output summary",
    "security:patch": "snyk wizard",
    "semantic-release": "semantic-release",
    "snyk-protect": "snyk protect",
    "snyk-test": "snyk test",
    "test": "nyc mocha 'tests/**/*.spec.js'",
    "test:watch": "nodemon --exec npm run test",
    "travis-deploy-once": "travis-deploy-once"
  },
  "snyk": true
}
