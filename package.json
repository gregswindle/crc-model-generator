{
  "name": "eslint-plugin-crc",
  "description": "Analyze and refactor JavaScript codebases with auto-generated Class-Responsibility-Collaboration models.",
  "version": "0.2.0",
  "author": {
    "name": "Greg Swindle",
    "email": "greg@swindle.net",
    "url": "http://swindle.net"
  },
  "bugs": {
    "url": "https://github.com/gregswindle/eslint-plugin-crc/issues"
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "build",
      "chore",
      "ci",
      "docs",
      "feat",
      "fix",
      "perf",
      "refactor",
      "revert",
      "style",
      "test"
    ],
    "scope": "\\S+.*"
  },
  "contributors": [],
  "dependencies": {
    "@semantic-release/condition-travis": "6.2.1",
    "agent-base": "4.1.2",
    "ajv": "5.4.0",
    "async": "2.6.0",
    "caniuse-lite": "1.0.30000769",
    "clone": "2.1.1",
    "color-convert": "1.9.1",
    "concat": "1.0.3",
    "conventional-changelog": "1.1.7",
    "conventional-recommended-bump": "1.0.3",
    "copy-concurrently": "1.0.5",
    "diff": "3.4.0",
    "eslint": "4.11.0",
    "espree": "3.5.2",
    "estraverse": "4.2.0",
    "global": "4.3.2",
    "global-dirs": "0.1.1",
    "ini": "1.3.5",
    "lodash": "4.17.4",
    "minami": "1.2.3",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "npm-packlist": "1.1.10",
    "npm-profile": "2.0.5",
    "pacote": "7.0.1",
    "pump": "1.0.3",
    "query-string": "5.0.1",
    "relative-path": "1.1.0",
    "require-dir": "0.3.2",
    "requireindex": "1.1.0",
    "shebang-loader": "0.0.1",
    "stream-each": "1.2.2",
    "type-detect": "4.0.5",
    "worker-farm": "1.5.2"
  },
  "devDependencies": {
    "babel-register": "6.26.0",
    "bithound": "1.7.0",
    "chai": "4.1.2",
    "codacy-coverage": "2.0.3",
    "codeclimate-test-reporter": "0.5.0",
    "commitplease": "3.1.0",
    "coveralls": "3.0.0",
    "dedupe": "2.1.0",
    "dirty-chai": "2.0.1",
    "docdash": "0.4.0",
    "esdoc-external-nodejs-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint-plugin-security": "1.4.0",
    "jsdoc": "3.5.5",
    "markdown-magic": "0.1.19",
    "markdown-magic-dependency-table": "1.3.2",
    "markdown-magic-install-command": "1.3.1",
    "markdown-magic-package-scripts": "1.2.1",
    "mocha": "4.0.1",
    "mocha-junit-reporter": "1.15.0",
    "nodemon": "1.12.1",
    "npmdoc-nsp": "2017.4.23",
    "nsp": "3.1.0",
    "nyc": "11.3.0",
    "properties-parser": "0.3.1",
    "semantic-release": "8.2.0",
    "semantic-release-cli": "3.2.5",
    "sinon": "4.1.2",
    "snyk": "1.49.4",
    "standard-version": "4.2.0",
    "uglify-js": "3.1.10"
  },
  "engines": {
    "node": ">=6"
  },
  "homepage": "https://github.com/gregswindle/eslint-plugin-crc/",
  "keywords": [
    "ast",
    "cli",
    "code-analysis",
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "javascript",
    "javascript-tools",
    "refactor",
    "refactoring",
    "refactoring-tools"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "**/fixtures/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "check-coverage": true,
    "lines": 100,
    "require": [
      "babel-register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gregswindle/eslint-plugin-crc.git"
  },
  "scripts": {
    "changelog": "./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s",
    "coverage": "./node_modules/.bin/nyc report",
    "coverage:all": "npm run coverage:codacy && npm run coverage:codecov && npm run coverage:coveralls",
    "coverage:codacy": "./node_modules/.bin/nyc report && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "coverage:codecov": "./node_modules/.bin/nyc report --reporter=text-lcov > ./coverage/coverage.lcov && ./node_modules/.bin/codecov `-t $CODECOV_TOKEN`",
    "coverage:coveralls": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "docs:api": "documentation build lib/rules --config docs/documentation.yml -f html -o docs/api/documentation",
    "docs:esdoc": "esdoc",
    "docs:markdown": "md-magic --path '{.github/*.md,**/*.md}' '**/*.md' --ignore 'node_modules' --config ./markdown.config.js",
    "docs:script": "node ./markdown.config.js",
    "docs": "npm run docs2html && npm run docs2md && ./node_modules/.bin/eslint -f html lib > docs/eslint-report.html",
    "docs2html": "./node_modules/.bin/jsdoc -c jsdoc.conf.json",
    "docs2md": "jsdoc2md  --template ../eslint-plugin-crc.wiki/README.hbs --files ./lib/rules/*.js > ../eslint-plugin-crc.wiki/API.md && markdown-toc -i ../eslint-plugin-crc.wiki/API.md",
    "posttest": "./node_modules/.bin/nsp check --output summary",
    "postversion": "npm run changelog && node ./.ci/bump/ && git add sonar-project.properties && git commit --amend && git push && git push --tags",
    "prepublish": "npm run snyk-protect",
    "pretest": "./node_modules/.bin/eslint . --fix",
    "preversion": "npm test && ./node_modules/.bin/nsp check --output summary",
    "push": "git push origin develop && git checkout master && git merge develop && git push origin master && git checkout develop && git status",
    "release": "standard-version",
    "security": "./node_modules/.bin/nsp check --output summary",
    "security:patch": "snyk wizard",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "sonar": "sonar-scanner -Dproject.settings=./sonar-project.properties",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha ./tests/lib/rules/*.js",
    "version": null,
    "watch:test": "./node_modules/.bin/nodemon -w ./tests -w ./lib -e js,json -x npm test",
    "snyk-protect": "snyk protect",
    "snyk-test": "snyk test"
  },
  "snyk": true
}
