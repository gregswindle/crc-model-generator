{
  "name": "eslint-plugin-crc",
  "description": "Document, analyze, and refactor JavaScript codebases with auto-generated CRC (Class-Responsibility-Collaborator) Models.",
  "version": "0.1.5-development",
  "author": {
    "name": "Greg Swindle",
    "email": "greg@swindle.net",
    "url": "https://github.com/gregswindle"
  },
  "bin": "lib/cli.js",
  "bugs": {
    "url": "https://github.com/gregswindle/eslint-plugin-crc/issues"
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "build",
      "chore",
      "ci",
      "docs",
      "feat",
      "fix",
      "perf",
      "refactor",
      "revert",
      "style",
      "test"
    ],
    "scope": "\\S+.*"
  },
  "contributors": [],
  "dependencies": {
    "bunyan": "1.8.12",
    "chalk": "2.3.1",
    "compromise": "11.4.0",
    "doctrine": "2.1.0",
    "escomplex": "2.0.0-alpha",
    "eslint": "4.17.0",
    "eslint-scope": "3.7.1",
    "espree": "3.5.3",
    "esquery": "1.0.0",
    "estraverse": "4.2.0",
    "fs-extra": "5.0.0",
    "lodash": "4.17.5",
    "markdown-toc": "1.2.0",
    "meow": "4.0.0",
    "requireindex": "1.2.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "1.0.1",
    "@semantic-release/git": "3.0.1",
    "@semantic-release/npm": "3.0.2",
    "chai": "4.1.2",
    "common-vocabulary": "1.0.1",
    "docdash": "0.4.0",
    "eslint-config-crockford": "2.0.0",
    "eslint-config-scanjs": "1.0.0-beta4",
    "eslint-config-xo-space": "0.18.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsdoc": "3.4.1",
    "eslint-plugin-node": "6.0.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-scanjs-rules": "0.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-plugin-unicorn": "4.0.2",
    "eslint-plugin-xss": "0.1.9",
    "jsdoc": "3.5.5",
    "lec": "^1.0.1",
    "markdown-magic": "0.1.20",
    "markdown-magic-dependency-table": "1.3.2",
    "markdown-magic-package-scripts": "1.2.1",
    "mocha": "5.0.1",
    "nodemon": "1.15.0",
    "nsp": "3.2.1",
    "nyc": "11.4.1",
    "properties-parser": "0.3.1",
    "semantic-release": "^12.4.1",
    "sinon": "4.3.0",
    "sinon-chai": "2.14.0",
    "snyk": "1.69.7",
    "travis-deploy-once": "^4.3.4"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "homepage": "https://github.com/gregswindle/eslint-plugin-crc/",
  "keywords": [
    "ast",
    "cli",
    "code-analysis",
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "javascript",
    "javascript-tools",
    "refactor",
    "refactoring",
    "refactoring-tools"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "exclude": [
      "**/descriptors/**",
      "**/tests/**",
      "reports/**"
    ],
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "./docs/reports/coverage"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "publish": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gregswindle/eslint-plugin-crc.git"
  },
  "scripts": {
    "docs": "npm run docs2html && npm run docs:markdown",
    "docs2html": "jsdoc -c config/jsdoc.conf.json -P package.json",
    "docs:glossary": "common-vocabulary",
    "docs:markdown": "md-magic --path '{.github/*.md,**/*.md}' '**/*.md' --ignore 'node_modules' --config ./config/markdown.config.js",
    "docs:script": "node ./markdown.config.js",
    "lint": "./node_modules/.bin/eslint . --fix",
    "postversion": "node ./.ci/bump/ && git add sonar-project.properties && git commit --amend && git push && git push --tags",
    "prepublishOnly": "lec lib/cli.js -c LF && npm run security",
    "pretest": "npm run security && npm run lint",
    "preversion": "npm test && npm run security",
    "push": "git push origin master && git status",
    "security": "./node_modules/.bin/nsp check --output summary",
    "security:patch": "snyk wizard",
    "semantic-release": "semantic-release",
    "snyk-protect": "snyk protect",
    "snyk-test": "snyk test",
    "sonar": "sonar-scanner -Dproject.settings=./sonar-project.properties",
    "test": "nyc mocha 'tests/**/*.spec.js'",
    "travis-deploy-once": "travis-deploy-once",
    "version": null
  },
  "snyk": true
}
